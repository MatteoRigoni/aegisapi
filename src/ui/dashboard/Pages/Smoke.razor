@page "/chart-smoke"


<MudChart ChartType="ChartType.Line"
          CustomPalette="@(new[] { "#ff6b6b" })"
          XAxisLabels="@XLabels"
          ChartSeries="@Series"
          Width="100%" Height="260px" />

@code {
    private string[] XLabels = { "Jan", "Feb", "Mar", "Apr", "May", "Jun" };
    private List<ChartSeries> Series = new()
    {
        new() { Name = "Demo", Data = new double[] { 10, 25, 15, 30, 20, 35 } }
    };
}