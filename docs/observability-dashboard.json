      "type": "timeseries",
      "title": "HTTP Requests (RPS)",
      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by(route) (rate(demo_requests_total[$__rate_interval]))"
        }
