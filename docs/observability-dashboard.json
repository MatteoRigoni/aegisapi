      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Request Latency p95 (ms)",
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(demo_work_latency_ms_bucket[$__rate_interval])) by (le))"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Error Responses (4xx / 5xx)",
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_server_request_duration_seconds_count{status_code=~\"4..\"}[$__rate_interval]))",
          "legendFormat": "4xx"
        },
        {
          "refId": "B",
          "expr": "sum(rate(http_server_request_duration_seconds_count{status_code=~\"5..\"}[$__rate_interval]))",
          "legendFormat": "5xx"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    }
